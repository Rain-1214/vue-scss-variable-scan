{
    "name": "vue-scss-variable-scan",
    "displayName": "vue-scss-variable-scan",
    "description": "",
    "version": "0.0.1",
    "publisher": "Rain",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "vue-scss-variable-scan.cache",
                "title": "Cache SCSS variable"
            }
        ],
        "configuration": [
            {
              "title": "vue scss variable scan",
              "properties": {
                "vue-scss-variable-scan.globalPath": {
                  "type": "string",
                  "default": "src/**/*.scss"
                },
                "vue-scss-variable-scan.globalExcludePath": {
                  "type": "string",
                  "default": ""
                },
                "vue-scss-variable-scan.enableEmmetSupport": {
                  "type": "boolean",
                  "default": false
                }
              }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
